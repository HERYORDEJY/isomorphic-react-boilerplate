{
  "name": "react-minimal-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server/index.js",
  "scripts": {
    "start": "node ./dist/server/index.js",
    "server": "rm -rf dist && webpack --config webpack.dev.config.babel.js; cross-env NODE_ENV=development DEBUG=api node ./dist/server/index.js",
    "server:prod": "rm -rf dist && cross-env NODE_ENV=production webpack --config webpack.prod.config.babel.js; cross-env NODE_ENV=production node ./dist/server/index.js",
    "watch": "rm -rf dist && webpack --watch --config webpack.dev.config.babel.js",
    "nodemon": "cross-env NODE_ENV=development DEBUG=api nodemon ./dist/server/index.js",
    "build": "rm -rf dist && webpack --config webpack.dev.config.babel.js --display-error-details",
    "build:static": "rm -rf dist && cross-env NODE_ENV=production webpack --config webpack.static.config.babel.js",
    "build:prod": "rm -rf dist && cross-env NODE_ENV=production webpack --config webpack.prod.config.babel.js",
    "lint": "node_modules/.bin/eslint ./src --fix"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "bootstrap-sass": "^3.3.7",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.7.2",
    "pixrem": "^4.0.1",
    "postcss-loader": "^2.0.9",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-hot-loader": "^3.1.3",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1",
    "webpack-dev-middleware": "^1.12.1",
    "webpack-hot-middleware": "^2.20.0"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "basic-auth": "^2.0.0",
    "body-parser": "^1.18.2",
    "compression": "^1.7.1",
    "debug": "^3.1.0",
    "express": "^4.16.2",
    "helmet": "^3.9.0",
    "serve-static": "^1.13.1"
  }
}
